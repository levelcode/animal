@charset "UTF-8";
/* CSS Document */
.main_like
{
	width: 810px;
	height: 400px;
	position: relative;
	padding: 175px 0px 0px;
}
.main_like .text_like
{
	width: 360px;
	height: 185px;
	position: relative;
	margin-left:35px; 
}
.main_like .text_like h1
{
	color: #8fbd46;
	font-size: 40px;
}
.main_like .text_like h2
{
	color: #fff;
	font-size: 54px;
	text-transform: none;
}
.main_like .text_like p
{
	color: #042656;
	font-size: 20px;
	line-height: 30px;
	margin-top: 45px;
}